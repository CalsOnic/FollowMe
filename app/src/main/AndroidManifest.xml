<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.idyll.follow" >

    <application android:allowBackup="true"
        android:name="com.idyll.follow.common.application.MainApplication"
        >

        <activity
            android:name="com.idyll.follow.iu.activity.IUActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.idyll.follow.music.activity.MusicListActivity"
            android:label="@string/title_activity_music_list" >
        </activity>
        <activity
            android:name="com.idyll.follow.music.activity.AnalyzeActivity"
            android:label="@string/title_activity_analyze" >
        </activity>

        <uses-permission android:name="anroid.permission.RECORD_AUDIO"/>
    </application>

</manifest>
